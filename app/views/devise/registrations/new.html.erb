<%= stylesheet_link_tag 'registrations', media: 'all', 'data-turbolinks-track': 'reload' %>
<div class="register">
  <h2 style= "margin-bottom: 30px">会員登録</h2>

  <div id="contact_form">
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= render "devise/shared/error_messages", resource: resource %>
          <%= f.email_field :email, autofocus: true, autocomplete: "email", id: "inputEmail", class: "form-control",:placeholder=>"email" ,style: "margin-bottom: 30px" %>
      <div class="button01">
        <%= f.submit "登録", class:"w-25 btn btn-lg btn-primary"%>
      </div>
    <% end %>
  </div>

  <div class="links">
  <%= render "devise/shared/links" %>
  </div>
</div>edew